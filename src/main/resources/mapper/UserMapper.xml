<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mj.jw.user.dao.UserMapper">
    <select id="getUserList" parameterType="map" resultType="com.mj.jw.user.entity.User">
    select id id,name name,sex sex,age age
